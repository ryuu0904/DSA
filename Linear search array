#include <iostream>
using namespace std;

int main(){
    int arr[10],n,i,key;
    
    cout<<"Enter the size of the array: ";
    cin>>n;
    
    cout<<"Enter "<<n<<" Elements in the array: ";
    for(i=0; i<n; i++){
        cin>>arr[i];
    }
    
    cout<<"Enter the element to be searched: ";
    cin>>key;
    
    for(i=0; i<n; i++){
        if(arr[i]==key){
            cout<<"Element found at index "<<i<<endl;
            break;
        }
    }
    if(i==n)
        cout<<"ELement not found!";
}
