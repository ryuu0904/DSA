#include <iostream>

using namespace std;
int main(){
    int arr[10],n,i;
    
    cout<< "Enter the size of the array: ";
    cin>> n;
    
    cout<< "Enter "<<n<<" array elements: ";
    for(i = 0; i<n; i++){
        cin>>arr[i];
    }
    cout<<"Array: ";
    for(i=0;i<n;i++){
        cout<< arr[i]<<" ";
    }

    n--;
    cout<<"\nArray after deleting the last element: ";
    for(i=0;i<n;i++){
        cout<< arr[i]<<" ";
    }
    
}
